<!--pages/orderDetails/sellGoods/index.wxml-->
<wxs module="filter" src="../../../common/wxs/filters.wxs"></wxs>
<view class='container'>

  <view class='product-container'>
    <block wx:for="{{products}}" wx:key="">
      <view class="product-item">
        <view class="item-left">
          <image src="{{item.send_avataUrl}}"></image>
          <image src="{{item.product_img}}"></image>
        </view>
        <view class="item-middle">
          <view>订单编号：{{item.id}}</view>
          <view>订单名称：{{item.name}}</view>
          <view>订单价格：{{item.price}}</view>
          <view>发送用户名：{{item.send_name}}</view>
        </view>


      </view>
    </block>
  </view>
  
  <view class='confirm-container'>

    <view class='confirm' bindtap="back">
      <text>返回</text>
    </view>

  </view>

</view>

